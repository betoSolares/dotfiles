#!/bin/sh

initializeANSI() {
	esc=""

	Bf="${esc}[30m"
	rf="${esc}[31m"
	gf="${esc}[32m"
	yf="${esc}[33m" bf="${esc}[34m"
	pf="${esc}[35m"
	cf="${esc}[36m"

	ON="${esc}[1m"
	OFF="${esc}[22m"

	reset="${esc}[0m"
}

initializeANSI

